rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Allow public read access to generated images
    match /generated/{allPaths=**} {
      allow read: if true;
      allow write: if false;
    }

    // Deny all other access
    match /{allPaths=**} {
      allow read, write: if false;
    }
  }
}
